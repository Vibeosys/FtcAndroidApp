<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vibeosys.tradenow">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.vibeosys.tradenow.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.vibeosys.tradenow.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vibeosys.tradenow.activities.RegisterActivity" />
        <activity android:name="com.vibeosys.tradenow.activities.ForgotPassActivity" />
        <activity
            android:name="com.vibeosys.tradenow.activities.TreadingAlertActivity"
            android:parentActivityName="com.vibeosys.tradenow.MainActivity" />
        <activity
            android:name="com.vibeosys.tradenow.activities.MyProfileActivity"
            android:parentActivityName="com.vibeosys.tradenow.MainActivity" />
        <activity android:name="com.vibeosys.tradenow.activities.LoginClientActivity" />
        <activity
            android:name="com.vibeosys.tradenow.activities.ResetPassActivity"
            android:parentActivityName="com.vibeosys.tradenow.MainActivity" />
        <activity android:name="com.vibeosys.tradenow.activities.DemoActivity"></activity>
    </application>

</manifest>